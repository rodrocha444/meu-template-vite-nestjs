    "date-fns": "^4.1.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
