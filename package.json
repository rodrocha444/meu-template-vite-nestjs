{
  "name": "meu-projeto",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "db:up": "docker compose up -d",
    "db:wait": "wait-on tcp:5432 --timeout 30000",
    "db:down": "docker compose down",
    "dev": "pnpm db:up && pnpm db:wait && turbo run dev --log-prefix=none",
    "clean": "rm -rf node_modules **/*/node_modules"
  },
  "author": "Rodrigo Rocha",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "prettier": "^3.8.1",
    "turbo": "^2.8.1",
    "wait-on": "^9.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@apollo/protobufjs",
      "@nestjs/core",
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
